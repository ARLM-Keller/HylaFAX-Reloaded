*Feature: ColorMode
{
    *rcNameID: =DL_COLOR_DISPLAY
    *DefaultOption: COLOR
    *Option: GrayScale
    {
        *rcNameID: =DL_MONO_DISPLAY
        *DevNumOfPlanes: 1
        *DevBPP: 24
        *DrvBPP: 24
        *Color?: FALSE
        *PaletteSize:1
        *PaletteProgrammable?:TRUE
        *Command:CmdDefinePaletteEntry{*Cmd:""}

        *switch: PrintMode
        {
            *case: HIGHSPEED
            {
                *Command: CmdSelect
                {
                    *Order: JOB_SETUP.5
                    *Cmd: =DL_GRAY_HIGHSPEED
                }
            }
            *case: HIGHQUALITY
            {
                *Command: CmdSelect
                {
                    *Order: JOB_SETUP.5
                    *Cmd: =DL_GRAY_HIGHQUALITY
                }
            }
        }
        *Constraints: LIST(Trapping.ON)
    }
    *Option: COLOR
    {
        *rcNameID: =DL_TRUECOLOR_DISPLAY
        *DevNumOfPlanes: 1
        *DevBPP: 24
        *DrvBPP: 24
        *PaletteSize:256
        *PaletteProgrammable?:TRUE
        *Command:CmdDefinePaletteEntry{*Cmd:""}

        *switch: PrintMode
        {
            *case: HIGHSPEED
            {
                *Command: CmdSelect
                {
                    *Order: JOB_SETUP.5
                    *Cmd: =DL_COLOR_HIGHSPEED
                }
            }
            *case: HIGHQUALITY
            {
                *Command: CmdSelect
                {
                    *Order: JOB_SETUP.5
                    *Cmd: =DL_COLOR_HIGHQUALITY
                }
            }
        }
    }
}

